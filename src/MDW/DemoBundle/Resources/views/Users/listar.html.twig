<h1>Listado de Usuarios</h1>
<table border="1">
    <tr>
        <th>ID</th>
        <th>Login</th>
        <th>Pass</th>
    </tr>
    {% for user in users %}
    <tr>
        <td>{{ user.id }}</td>
        <td>{{ user.login }}</td>
        <td>{{ user.password }}</td>
		<td>{{ user.email }}</td>
    </tr>
    {% endfor %}
</table>