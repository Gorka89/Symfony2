<!DOCTYPE html>
<html>
    <head>
	{% block head %}
		<meta charset="UTF-8" />
		<title>{% block title %}Servicios{% endblock %}</title>
		{% block stylesheets %}
			 <link href="{{ asset('bundles/mdwdemo/css/style.css') }}" type="text/css" rel="stylesheet" />
		{% endblock %}
    {% endblock %}
    </head>
    <body>
	{% block body %}
		<div id="header">{% block blockimage %}{% endblock %}</div>
		<div id="leftpanel">{% block leftPanel %}{% endblock %}</div>
        <div id="content">{% block content %}{% endblock %}</div>
		
        <div id="footer">
            {% block footer %}
                Footer
            {% endblock %}
        </div>
		
		{% block javascripts %}{% endblock %}
	{% endblock %}
    </body>
</html>